<?xml version="1.0"?>
<package>
  <name>rviz_ugr_plugin</name>
  <version>0.11.0</version>
  <description>An rviz plugin to visualise Observation msgs</description>

  <maintainer email="lowiek.vandenstockt@ugentracing.be">Lowiek Van den Stockt</maintainer>

  <author>Lowiek Van den Stockt</author>
  <license>UNKNOWN</license>
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>rviz</build_depend>
  <build_depend>ugr_msgs</build_depend>

  <run_depend>rviz</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>visualization_msgs</run_depend>

  <export>
    <rviz plugin="${prefix}/plugin_description.xml"/>
</export>

</package>