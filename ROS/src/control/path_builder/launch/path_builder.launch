<launch>
    <node pkg="path_builder" type="path_builder.py" name="path_builder" respawn="true" output="screen">
        <param name="world_frame" value="ugr/map" />
        <param name="max_distance_away_from_start" value="9" />
        <param name="min_distance_away_from_start" value="16" />

        <remap from="/input/path" to="/pathplanning/poses"/>
        <remap from="/output/path" to="/pathplanning/global_path"/>
        <remap from="/output/vis_path" to="/pathplanning/vis_path"/>
    </node>
</launch>